{
  "name": "webgl-playground",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "env": "env",
    "lint": "jshint **.js",
    "test": "echo \"Error: no test specified\" && exit 1",
    "clean": "rm -rf dist/*",
